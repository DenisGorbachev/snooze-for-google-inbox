{
  "manifest_version": 2,
  "name": "Snooze",
  "description": "More snooze times for Inbox by Gmail",
  "version": "1.0",
  // arrive.js requires Chrome 27+
  "minimum_chrome_version": "27.0",
  "content_scripts": [
    {
      "matches": ["https://inbox.google.com/*"],
      "css": ["main.css"],
      "js": ["underscore.js", "moment.js", "bililiteRange.js", "jquery.js", "jquery.simulate.js", "jquery.sendkeys.js", "arrive.js", "cl.js", "main.js"],
      "run_at": "document_end"
    }
  ]
}
